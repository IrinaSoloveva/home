{
"validate": [
		{"id": "nameUser", "pattern": "^[a-zA-Zа-яА-ЯёЁ]+$", "errorMessage": "Неверный формат. Допустимы только буквы"},
		{"id": "cityUser", "pattern": "^[a-zA-Zа-яА-ЯёЁ]+$", "errorMessage": "Неверный формат. Допустимы только буквы"},
